<body>
  <main style="margin: 1em;">
    <h3>Create Account</h3>
      <form action="/register" method="POST">
      <div>
        <label for="email"> Email </label>
        <input type="email" name="email">
      </div>
      <div>
        <label for="password"> Password </label>
        <input type="password"  name="password">
